<?xml version="1.0" encoding="utf-8"?>
<openerp>
	<data>
		<record id="setirOperationForm" model="ir.ui.view">
			<field name="name">setirOperationForm</field>
			<field name="model">setir.operation</field>
			<field name="priority">4</field>
			<field name="arch" type="xml">
				<form string="Operation">
					<sheet>
	                    <div class="oe_title">
	                        <h1>
	                            <field name="name" readonly="1"/>
	                        </h1>
	                    </div>
						<!-- label autofocus="autofocus" for="file_export6">Archivo:</label -->
						<!--  input accept=".csv, .xls, .xlsx, .ods" class="oe_import_file" id-attf-id="file_#{_id}" name="fileB" filename="strFilename" type="file"/ -->
						<field class="oe_import_file" name="binaryFile" filename="strFile" type="file"/>
						<field name="strFile"/>
					</sheet>
					<div class="oe_chatter">
						<field name="message_follower_ids" widget="mail_followers"/>
						<field name="message_ids" widget="mail_thread"/>
					</div>
				</form>
			</field>
		</record>
	</data>
</openerp>